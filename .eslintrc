{
  "root": true,
  "parser": "babel-eslint",
  "parserOptions": {
    // Required for certain syntax usages
    "ecmaVersion": 2020
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb",
    "plugin:jsx-a11y/recommended",
    "prettier",
    "prettier/react"
  ],
  "plugins": [
    "jsx-a11y",
    "prettier",
    "react"
  ],
  "env": {
    "browser": true
  },
  "rules": {
    "react/prop-types": 0,
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "semi": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "prettier/prettier": [
      "error", {
        "semi": false
      }
    ],
    "no-console": "off"
  }
}